@model DAIF2020.TSM.Models.DataModels.TSMGame

@{
    ViewData["Title"] = "Edit";
}

<h1>Edit</h1>

<h4>TSMGame</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="Id" />
            <div class="form-group">
                <label asp-for="GameDateTime" class="control-label"></label>
                <input asp-for="GameDateTime" class="form-control" />
                <span asp-validation-for="GameDateTime" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="WeekDay" class="control-label"></label>
                <input asp-for="WeekDay" class="form-control" />
                <span asp-validation-for="WeekDay" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="GameNumber" class="control-label"></label>
                <input asp-for="GameNumber" class="form-control" />
                <span asp-validation-for="GameNumber" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Round" class="control-label"></label>
                <input asp-for="Round" class="form-control" />
                <span asp-validation-for="Round" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="HomeTeam" class="control-label"></label>
                <input asp-for="HomeTeam" class="form-control" />
                <span asp-validation-for="HomeTeam" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="AwayTeam" class="control-label"></label>
                <input asp-for="AwayTeam" class="form-control" />
                <span asp-validation-for="AwayTeam" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Arena" class="control-label"></label>
                <input asp-for="Arena" class="form-control" />
                <span asp-validation-for="Arena" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Series" class="control-label"></label>
                <input asp-for="Series" class="form-control" />
                <span asp-validation-for="Series" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="HD1" class="control-label"></label>
                <input asp-for="HD1" class="form-control" />
                <span asp-validation-for="HD1" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="HD2" class="control-label"></label>
                <input asp-for="HD2" class="form-control" />
                <span asp-validation-for="HD2" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="LD1" class="control-label"></label>
                <input asp-for="LD1" class="form-control" />
                <span asp-validation-for="LD1" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="LD2" class="control-label"></label>
                <input asp-for="LD2" class="form-control" />
                <span asp-validation-for="LD2" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Supervisor" class="control-label"></label>
                <input asp-for="Supervisor" class="form-control" />
                <span asp-validation-for="Supervisor" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="DateChanged" class="control-label"></label>
                <input asp-for="DateChanged" class="form-control" />
                <span asp-validation-for="DateChanged" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="ChangedBy" class="control-label"></label>
                <input asp-for="ChangedBy" class="form-control" />
                <span asp-validation-for="ChangedBy" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="TSMGameStatusId" class="control-label"></label>
                <select asp-for="TSMGameStatusId" class="form-control" asp-items="ViewBag.TSMGameStatusId"></select>
                <span asp-validation-for="TSMGameStatusId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-sm btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a class="btn btn-sm btn-success" asp-controller="TSMGames" asp-action="ListTSMGames">Back to TSMGames</a>
</div>

