@model IEnumerable<DAIF2020.TSM.Models.DataModels.Game20192020>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.TSMGameStatus)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.GameDateTime)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.GameLength)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.WeekDay)
