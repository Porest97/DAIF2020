@model IEnumerable<DAIF2020.Models.DataModels.ArchivedGame>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.DateTime)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.GameNumber)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.GameCetegory)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.GameStatus)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.GameType)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Arena)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.HomeTeam)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AwayTeam)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Score)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.HD1)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.HD2)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LD1)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LD2)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ParticipatedTime)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Person)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.DateTime)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.GameNumber)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.GameCetegory)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.GameStatus)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.GameType)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Arena)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.HomeTeam)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AwayTeam)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Score)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.HD1)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.HD2)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LD1)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LD2)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ParticipatedTime)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Person.Id)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
