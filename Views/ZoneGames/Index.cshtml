            <td>
                @Html.DisplayFor(modelItem => item.AwayTeam2.ClubName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UDZ2.FullName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Supervisor.FullName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ZoneGameDateTime)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Arena.ArenaName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ZoneGameName)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
