                @Html.DisplayFor(modelItem => item.AwayTeam1.ClubName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UDZ1.FullName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Zone2.ZoneName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TSMNumberZone2)
            </td>
