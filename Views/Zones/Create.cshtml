@model DAIF2020.Models.SettingModels.Zone

@{
    ViewData["Title"] = "Create";
}

<h1>Create</h1>

<h4>Zone</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="ZoneName" class="control-label"></label>
                <input asp-for="ZoneName" class="form-control" />
                <span asp-validation-for="ZoneName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-sm btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a class="btn btn-sm btn-success" asp-action="ListZones">Back to Zones</a>
</div>

